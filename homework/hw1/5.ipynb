      "source": [
        "# Функция, которая принимает список чисел и возвращает их сумму.\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 1,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "bOWAErCwQRlg",
        "outputId": "d770be41-4f5d-475b-bd9b-705041c2232c"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "Input data:  [1, 2, 3, 0]\n",
            "Result:  6\n"
          ]
        }
      ],
      "source": [
        "x = [1,2,3,0]\n",
        "print(\"Input data: \", x)\n",
        "def total_sum(x):\n",
        "    length = len(x)\n",
        "    sum = 0\n",
        "    c = 0\n",
        "    while c < length:\n",
        "       sum = sum + x[c]\n",
        "       c += 1\n",
        "    return sum\n",
        "\n",
        "print(\"Result: \", total_sum(x))"
      ]
    }
  ],
